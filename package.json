{
  "name": "myapp-financial",
  "version": "1.0.0",
  "description": "A financial management app built with Expo, React Native, and TypeScript",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "expo": "^51.0.0",
    "expo-font": "^12.0.0",
    "expo-splash-screen": "^0.27.0",
    "expo-status-bar": "^1.12.0",
    "expo-linear-gradient": "^13.0.0",
    "expo-notifications": "^0.27.0",
    "react": "^18.2.0",
    "react-native": "^0.74.0",
    "react-native-screens": "^3.31.0",
    "react-native-safe-area-context": "^4.8.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-reanimated": "^3.10.0",
    "react-native-vector-icons": "^10.0.0",
    "@react-navigation/native": "^6.1.0",
    "@react-navigation/bottom-tabs": "^6.5.0",
    "@react-navigation/stack": "^6.3.0",
    "@react-navigation/drawer": "^6.6.0",
    "typescript": "^5.3.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "zustand": "^4.4.0",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "react-native-chart-kit": "^6.12.0",
    "react-native-svg": "^13.14.0",
    "react-native-svg-charts": "^5.7.0",
    "@react-native-async-storage/async-storage": "^1.23.0",
    "react-native-keychain": "^8.1.0",
    "react-native-config": "^1.5.0",
    "react-hook-form": "^7.50.0",
    "yup": "^1.3.0",
    "@hookform/resolvers": "^3.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^51.0.0",
    "prettier": "^3.0.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.3.0"
  },
  "expo": {
    "name": "MyApp Financial",
    "slug": "myapp-financial",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.dannylewis.myapp.financial"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dannylewis.myapp.financial"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-notifications"
    ]
  }
}
