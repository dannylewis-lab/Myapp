# Application Configuration
REACT_APP_NAME=MyApp
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG=false

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:3001/api
REACT_APP_API_TIMEOUT=30000
REACT_APP_API_RETRY_COUNT=3

# Authentication
REACT_APP_AUTH_URL=http://localhost:3001/auth
REACT_APP_TOKEN_STORAGE_KEY=myapp_token
REACT_APP_SESSION_TIMEOUT=3600000

# Database
REACT_APP_DB_HOST=localhost
REACT_APP_DB_PORT=5432
REACT_APP_DB_NAME=myapp_db
REACT_APP_DB_USER=postgres
REACT_APP_DB_PASSWORD=

# Security
REACT_APP_ENCRYPTION_ENABLED=true
REACT_APP_ENCRYPTION_KEY=
REACT_APP_CORS_ENABLED=true

# Third-party Services
REACT_APP_ANALYTICS_ENABLED=false
REACT_APP_ANALYTICS_ID=
REACT_APP_SENTRY_DSN=

# Email Configuration
REACT_APP_SMTP_HOST=smtp.gmail.com
REACT_APP_SMTP_PORT=587
REACT_APP_SMTP_USER=
REACT_APP_SMTP_PASSWORD=
REACT_APP_SMTP_FROM=noreply@myapp.com

# Features
REACT_APP_FEATURE_EXPORT_PDF=true
REACT_APP_FEATURE_MULTI_CURRENCY=true
REACT_APP_FEATURE_NOTIFICATIONS=true
REACT_APP_FEATURE_DARK_MODE=true

# Logging
REACT_APP_LOG_LEVEL=info
REACT_APP_LOG_FORMAT=json
